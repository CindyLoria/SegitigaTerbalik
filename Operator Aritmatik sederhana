#include<iostream>
using namespace std;

int main() {
   
    int A;
    int B;
    
    cout<<"Masukkan angka pertama: "<<endl;
    cin>>A;
    cout<<"Masukkan angka kedua: "<<endl;
    cin>>B;
    
    cout<<"Hasil Penjumlahan: "<< A + B <<endl;
    cout<<"Hasil Pengurangan: "<< A - B <<endl;
    cout<<"Hasil Perkalian  : "<< A*B <<endl;
    
    return 0;
}
