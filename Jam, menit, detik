#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    
    int s, jam, menit, detik,sisa;
    
    cin>>s;
    
    jam = s / 3600;
    sisa = s - (jam*3600);
    menit = sisa / 60;
    detik = sisa - (menit*60);
    cout<<jam<<" "<<"jam"<<" "<<menit<<" "<<"menit"<<" "<<detik<<" "<<"detik";
    
    return 0;
}
